# Production Profile Configuration
# High-security settings for production VNC access

# Connection Settings
VNC_HOSTNAME=vnc-prod.company.com
VNC_LOCAL_PORT=5902

# Security Settings
TUNNEL_LOGLEVEL=warn
TUNNEL_TRANSPORT_LOGLEVEL=error

# Performance Settings
CONTAINER_CPU_LIMIT=1.0
CONTAINER_MEMORY_LIMIT=512M
CONTAINER_CPU_RESERVATION=0.5
CONTAINER_MEMORY_RESERVATION=256M

# Stability Settings
TUNNEL_RETRIES=10
TUNNEL_GRACE_PERIOD=60s
RESTART_MAX_ATTEMPTS=5
HEALTHCHECK_INTERVAL=60s

# Monitoring
METRICS_PORT=2001
LOG_MAX_SIZE=50m
LOG_MAX_FILES=10